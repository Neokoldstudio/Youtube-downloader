<service>
	<id>youtubedownloader.exe</id>
	<name>Youtube Downloader</name>
	<description>A Server that allow you to download any video on youtube based on his extension</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>--harmony</argument>
	<argument>c:\Users\utilisateur\Desktop\Youtube downloader\Server\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\Users\utilisateur\Desktop\Youtube downloader\Server\index.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>Youtube Downloader wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<serviceaccount>
		<domain>PC-NEO</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>c:\Users\utilisateur\Desktop\Youtube downloader</workingdirectory>
</service>